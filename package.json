{
  "name": "react-webpack-2-app",
  "description": "Example",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "test:component:all": "mocha --require ignore-styles test/bootstrap.js test/app/**/*.test.jsx --reporter dot"
  },
  "betterScripts": {
    "build:dev": {
      "command": "node_modules\\.bin\\webpack --progress --watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "node_modules\\.bin\\webpack",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "classnames": "^2.2.5",
    "concurrently": "^3.2.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "beta",
    "gulp": "^3.9.1",
    "gulp-webserver": "^0.9.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.1",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "chai": "^3.5.0",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.7.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2"
  }
}
